{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "RG_Location": {
      "type": "string"
    },
    "RG_Name": {
      "type": "string"
    },
    "nicName": {
      "type": "string"
    },
    "publicIPAddressName": {
      "type": "string"
    },
    "virtualNetworkName": {
      "type": "string"
    },
    "subnet1Name": {
      "type": "string"
    },
    "NetworkSecurityGroupName": {
      "type": "string"
    }
  },
  "variables": {},
  "resources": [
    {
      "apiVersion": "2017-05-10",
      "name": "nicSingleDeployment",
      "type": "Microsoft.Resources/deployments",
      "resourceGroup": "[parameters('RG_Name')]",
      "dependsOn": [
      ],
      "properties": {
        "mode": "Incremental",
        "template": {
          "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
          "contentVersion": "1.0.0.0",
          "resources": [
            {
              "apiVersion": "2016-03-30",
              "type": "Microsoft.Network/networkInterfaces",
              "name": "[parameters('nicName')]",
              "location": "[parameters('RG_Location')]",
              "tags": {
                "displayName": "NetworkInterface",
                "createdBy": "Marc Kean",
                "Application": "eJukebox"
              },
              "properties": {
                "ipConfigurations": [
                  {
                    "name": "ipconfig1",
                    "properties": {
                      "privateIPAllocationMethod": "Dynamic",
                      "publicIPAddress": {
                        "id": "[resourceId('Microsoft.Network/publicIPAddresses', parameters('publicIPAddressName'))]"
                      },
                      "subnet": {
                        "id": "[resourceId('Microsoft.Network/virtualNetworks/subnets', parameters('virtualNetworkName'), parameters('subnet1Name'))]"
                      }
                    }
                  }
                ],
                "networkSecurityGroup": {
                  "id": "[resourceId('Microsoft.Network/networkSecurityGroups', parameters('NetworkSecurityGroupName'))]"
                }
              },
              "dependsOn": [

              ]
            }
          ]
        }
      }
    }
  ]
}