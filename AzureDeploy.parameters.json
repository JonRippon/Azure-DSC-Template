{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "adminUsername": {
      "value": "marc.kean"
    },
    "adminPassword": {
      "reference": {
        "keyVault": {
          "id": "/subscriptions/6bb00255-5486-4db1-96ca-5baefc18b0b2/resourceGroups/MSMarcVault/providers/Microsoft.KeyVault/vaults/MSMarcVault"
        },
        "secretName": "VMTestAdminPassword"
      }
    },
    "RG_Location": {
      "value": "australiaeast",
      "metadata": {
        "description": "Name of the Azure Region this is deployed to"
      }
    },
    "RG_Name": {
      "value": "DSCTestTemplate4",
      "metadata": {
        "description": "Name of the main resource group where the primary Resources are deployed to, e.g. the VM, NIC, Disks etc"
      }
    },
    "Shared_RG_Name": {
      "value": "SharedResources",
      "metadata": {
        "description": "Name of the resource group where Shared Resources are deployed to, e.g. the vNet (ignored if deployed into an existing vNet)"
      }
    },
    "DisksType": {
      "value": "Un-Managed",
      "allowedValues": [
        "Managed",
        "Un-Managed"
      ],
      "metadata": {
        "description": "Disk type, Managed or Un-Managed"
      }
    },
    "dataDiskCount": {
      "value": 1,
      "allowedvalues": [
        1,
        2,
        3,
        4
      ],
      "metadata": {
        "description": "Number of data disks"
      }
    },
    "Existing_vNet": {
      "value": "True",
      "allowedValues": [
        "True",
        "False"
      ],
      "metadata": {
        "description": "Deploy into an existing vNet or not"
      }
    },
    "virtualNetworkName": {
      "value": "eJukeNetwork",
      "metadata": {
        "description": "vNet to deploy into, or to create if Existing_vNet is false"
      }
    },
    "subnetName": {
      "value": "First",
      "metadata": {
        "description": "Subnet to deploy into, or to create if Existing_vNet is false"
      }
    },
    "VM_Name_Prefix": {
      "value": "eJukebox",
      "metadata": {
        "description": "Prefix for Computer Name"
      }
    },
    "VM_Name_Suffix": {
      "value": "1606",
      "metadata": {
        "description": "Suffix for Computer Name"
      }
    },
    "dataDiskCount": {
      "value": 1
    },
    "VNCKey": {
      "reference": {
        "keyVault": {
          "id": "/subscriptions/6bb00255-5486-4db1-96ca-5baefc18b0b2/resourceGroups/MSMarcVault/providers/Microsoft.KeyVault/vaults/MSMarcVault"
        },
        "secretName": "VNCKey"
      }
    }
  }
}